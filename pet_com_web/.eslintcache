er } from 'react-router-dom'\r\n\r\nclass GoodsDetail extends Component {\r\n  state = {\r\n    commodity_id: 1,\r\n    isPopup: false,\r\n    imgsUrl: [\r\n      'https://iconfont.alicdn.com/t/0c69570a-c540-4515-bf04-fa2c5461275a.png',\r\n      'https://iconfont.alicdn.com/t/f4159424-c5cf-4bda-860a-b54b34cb4614.png',\r\n      'https://iconfont.alicdn.com/t/d923bb17-0d09-4198-8ce9-9b793639b3dc.png'\r\n    ]\r\n  }\r\n\r\n  changeIsPopup= (e) => {\r\n    e.stopPropagation()\r\n    const {isPopup} = this.state\r\n    this.setState({isPopup: !isPopup})\r\n  }\r\n\r\n  handleScroll = (func) => {\r\n    const top = this.topDom.getBoundingClientRect().top\r\n    console.log('top', top)\r\n    if(top >= 0) {\r\n      console.log('隐藏')\r\n      func(false)\r\n    } else {\r\n      console.log('显示')\r\n      func(true)\r\n    }\r\n  }\r\n  \r\n  render() {\r\n    console.log('进入商品详细信息页面')\r\n    const {imgsUrl, isPopup, commodity_id} = this.state\r\n    return (\r\n      <div className=\"goods-detail-container\">\r\n        <Nav handleScroll={this.handleScroll}/>\r\n        <div ref={c => this.topDom = c} id=\"goods\" className=\"goods-detail-container-imgs\"><SwiperImg imgsUrl={imgsUrl}/></div>\r\n        <div className=\"bg padding1\">\r\n          <span className=\"font4 font-bolder font-theme\">￥ 709.00</span>\r\n          <div className=\"padding1-tb font-bold font3\">超级无敌炫酷至尊独享plus max deep extra商品介绍 炫酷喜洋洋马拉松</div>\r\n        </div>\r\n        <div className=\"margin1-t\">\r\n          <HandleBar\r\n            click={this.changeIsPopup}\r\n            className=\"padding1\"\r\n            leftSlot={\r\n              <span className=\"font-deep-gray\">选择</span>\r\n            }\r\n            centerSlot={\r\n              <span className=\"font25\">数量：1件</span>\r\n            }\r\n          />\r\n          <HandleBar\r\n            className=\"padding1\"\r\n            leftSlot={\r\n              <span className=\"font-deep-gray\">服务</span>\r\n            }\r\n            centerSlot={\r\n              <span className=\"font25\">正品保证,99元包邮</span>\r\n            }\r\n          />\r\n          <div id=\"goods-common\" className=\"margin1-t\">\r\n            <HandleBar \r\n              className=\"padding1\"\r\n              leftSlot={\r\n                <span className=\"font4\">评价 （17）</span>\r\n              }\r\n              rightSlot={\r\n                <span className=\"font-deep-gray\">查看更多</span>\r\n              }\r\n            />\r\n            <div>\r\n              <GoodsCommon />\r\n              <GoodsCommon />\r\n            </div>\r\n          </div>\r\n          <div id=\"goods-detail\" className=\"margin1-t bg padding1\">\r\n              <div className=\"font4\">详情</div>\r\n              <div className=\"padding1-tb\">\r\n                <img className=\"wh100\" src=\"https://iconfont.alicdn.com/t/72a90ceb-8aa1-400a-932e-4fa7f0f0daf1.png\" alt=\"商品详情信息\" />\r\n              </div>\r\n          </div>\r\n          <Footer />\r\n        </div>\r\n        <Popup isPopup={isPopup} popout={this.changeIsPopup} >\r\n          <div className=\"goods-detail-popup bg\">\r\n            <CommodityBar/>\r\n            <div className=\"margin1-tb flex flex-between padding1\">\r\n              <span>购买数量</span>\r\n              <Timer />\r\n            </div>\r\n            <div className=\"gd-buyButton\">\r\n              <BuyButton />\r\n            </div>\r\n          </div>\r\n        </Popup>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nexport default withRouter(GoodsDetail)\r\n","E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\GoodsDetail\\GoodsCommon\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\GoodsDetail\\Nav\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\components\\CommodityBar\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\components\\CommodityBar\\Timer\\index.jsx",["238"],"import React, { Component } from 'react'\r\nimport './index.less'\r\n\r\nexport default class Timer extends Component {\r\n  state = {\r\n    number: this.props.number || 1\r\n  }\r\n\r\n  sub = (e) => {\r\n    e.stopPropagation()\r\n    let {number} = this.state\r\n    if(number == 1) {\r\n      return \r\n    } else {\r\n      this.setState({number: --number}, () => this.props.setNewNum(number))\r\n    }\r\n  }\r\n\r\n  add = (e) => {\r\n    e.stopPropagation()\r\n    let {number} = this.state\r\n    this.setState({number: ++number}, () => this.props.setNewNum(number))\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"timer-container\">\r\n        <div onClick={this.sub} className=\"tc-sub\">-</div>\r\n        <div className=\"tc-number\">{this.state.number}</div>\r\n        <div onClick={this.add} className=\"tc-add\">+</div>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n","E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\FillOrder\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\FillOrder\\Bar\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\GoodsDetail\\BuyButton\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\GoodsDetail\\Footer\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Cart\\index.jsx",["239","240"],"import React, { Component } from 'react'\r\nimport NavBar from '@/components/NavBar'\r\nimport CommodityBar from '@/components/CommodityBar'\r\nimport Footer from './Footer'\r\nimport './index.less'\r\n\r\nexport default class Cart extends Component {\r\n  state = {\r\n    commodityList: [\r\n      {\r\n        id: 1,\r\n        name: '一号商品',\r\n        price: '1800.32',\r\n        number: 1,\r\n        checked: true\r\n      },\r\n      {\r\n        id: 2,\r\n        name: '一号商品',\r\n        price: '1800.95',\r\n        number: 2,\r\n        checked: false\r\n      },\r\n      {\r\n        id: 3,\r\n        name: '一号商品',\r\n        price: '1800.23',\r\n        number: 3,\r\n        checked: false\r\n      }\r\n    ],\r\n  }\r\n\r\n  setNewNum = (newNumber, id) => {\r\n    this.updateCommodityList(id, 'number', newNumber)\r\n  }\r\n\r\n  handleChecked = (e, id) =>{\r\n    this.updateCommodityList(id, 'checked', e.target.checked)\r\n  }\r\n\r\n  computedTotalPrice = () => {\r\n    let index = 0\r\n    const {commodityList} = this.state\r\n    const totalPrice = commodityList.map(commodity => {\r\n      if (commodity.checked) {\r\n        index++\r\n        return parseFloat(commodity.price) * commodity.number\r\n      } else {\r\n        return 0\r\n      }\r\n    }).reduce((total, cur) => {\r\n      console.log('index',index)\r\n      console.log('commodityList.length', commodityList.length)\r\n      return parseFloat(total) + parseFloat(cur)\r\n    })\r\n    return [totalPrice.toFixed(2), index == commodityList.length]\r\n  }\r\n\r\n  // 更新商品列表\r\n  updateCommodityList = (id, key, value) => {\r\n    const {commodityList} = this.state\r\n    const newCommodityList = commodityList.map(commodity => {\r\n      if (id) {\r\n        if (commodity.id == id) {\r\n          return Object.assign(commodity, {[key]:value})\r\n        } else {\r\n          return commodity\r\n        }\r\n      } else {\r\n        return Object.assign(commodity, {[key]:value})\r\n      }    \r\n    })\r\n    this.setState({commodityList:newCommodityList}, () => console.log('修改后',commodityList))\r\n  }\r\n\r\n  render() {\r\n    const {commodityList} = this.state\r\n    return (\r\n      <div>\r\n        <NavBar bgColor=\"red\" color=\"#fff\" title=\"购物车\"\r\n          rightSlot={\r\n            <span style={{color: '#fff'}}>编辑</span>\r\n          }\r\n        />\r\n        <div className=\"bg padding1\">\r\n          {\r\n            commodityList.map(commodity => {\r\n              return (\r\n                <div key={commodity.id} className=\"flex cart-commodity-bar\">\r\n                  <input onChange={(e) => this.handleChecked(e,commodity.id)} checked={commodity.checked} type=\"checkbox\" name=\"\" value=\"\"/>\r\n        