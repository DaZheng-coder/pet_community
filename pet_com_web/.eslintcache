r.slideTo(targetIdx,500, false)\r\n    this.setState({activeIndex:targetIdx})\r\n  }\r\n\r\n  render() {\r\n    /**\r\n     * 传入参数props contentList中必须为返回值为dom结构的函数\r\n     */\r\n    const {tabItems,contentList} = this.props\r\n    const {activeIndex,contentHeight} = this.state\r\n    const activeClass = this.props.activeClass || 'active'\r\n    return (\r\n      <div className=\"linkagebar-container\">\r\n        <div style={{top: this.props.height + 'px'}} className=\"tab-swiper-container bg hidden sticky\">\r\n          <ul className=\"swiper-wrapper\" onClick={this.onSlideTo}>\r\n            {\r\n              tabItems && \r\n                tabItems.map((tab,index) => \r\n                  <li className={`swiper-slide padding1-tb ${activeIndex == index && activeClass}`}  key={index} index={index}>{tab}</li>\r\n                )\r\n            }\r\n          </ul>\r\n        </div>\r\n        {/* <div className=\"mask\" style={{height: this.props.height + 'px'}}/> */}\r\n        <div className=\"content-swiper-container hidden\">\r\n          <div className=\"swiper-wrapper\">\r\n            {\r\n              contentList && \r\n                contentList.map((content,index) => \r\n                  <div className=\"swiper-slide\" style={{overflow:'scroll'}} key={index} index={index} onScroll={this.handleSlideScroll}>\r\n                    {content}\r\n                  </div>\r\n                )\r\n            }\r\n          </div>\r\n        </div>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n","E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Community\\Content\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Community\\Content\\UserCardList\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Community\\Content\\Topic\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Community\\Content\\PetClass\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Community\\Content\\Topic\\TopicItem\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\Community\\Content\\Topic\\TitleCard\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\utils\\index.js",["295"],"/**\r\n * 只执行一次的函数\r\n * @param {*} fn 执行的函数\r\n * @param {*} context 修改函数的this指向\r\n */\r\nexport function once(fn, context) {\r\n  let result\r\n  return function() {\r\n    if(fn) {\r\n      result = fn.apply(context || this, arguments)\r\n      fn = null\r\n    }\r\n    return result\r\n  }\r\n}\r\n\r\n/**\r\n * 是否是ios系统\r\n */\r\nexport function isIOS() {\r\n  var ua = navigator.userAgent.toLowerCase()\r\n  if(ua.match(/iPhone\\sOS/i) == \"iphone os\") {\r\n    return true\r\n  } else {\r\n    return false\r\n  }\r\n}\r\n\r\n/**\r\n  设置localStorage并且设置过期时间\r\n*/\r\n\r\nexport const localStorageSet = (name, data) => {\r\n  const obj = {\r\n    data,\r\n    expire: new Date().getTime() + 1000 * 60 * 30\r\n  }\r\n  localStorage.setItem(name, JSON.stringify(obj))\r\n}\r\n\r\n/**\r\n  获取localStorage中的值\r\n*/\r\nexport const localStorageGet = name => {\r\n  const storage = localStorage.getItem(name)\r\n  const time = new Date().getTime()\r\n  let result = null\r\n  if (storage) {\r\n    const obj = JSON.parse(storage)\r\n    if (time < obj.expire) {\r\n      result = obj.data\r\n    } else {\r\n      localStorage.removeItem(name)\r\n    }\r\n  }\r\n  return result\r\n}","E:\\ReactProjects\\pet_com\\pet_com_web\\src\\components\\SwiperImg\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\components\\Button\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\UserSetting\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\components\\HandleBar\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\pages\\DynamicDetail\\index.jsx",[],"E:\\ReactProjects\\pet_com\\pet_com_web\\src\\components\\Common\\index.jsx",["296"],"import React, { Component, Fragment } from 'react'\r\nimport Avatar from '@/components/Avatar'\r\nimport Popup from '@/co